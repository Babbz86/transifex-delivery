{
  "name": "transifex-delivery",
  "version": "0.5.0",
  "description": "Transifex Content Delivery Service",
  "keywords": [
    "transifex",
    "i18n",
    "l10n",
    "localization"
  ],
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "eslint": "eslint src/",
    "start": "node ./src/index.js",
    "start-dev": "node ./node_modules/nodemon/bin/nodemon -L -e js,yml --watch ./src --watch ./config ./src/index.js",
    "start-debugger": "node --inspect-brk=0.0.0.0 ./src/index.js",
    "test": "NODE_ENV=test nyc --reporter=text --all --reporter=html mocha --timeout 5000 --exit \"./tests/**/*.spec.js\"",
    "create-project": "node ./src/cli/create_project.js",
    "list-projects": "node ./src/cli/list_projects.js",
    "delete-project": "node ./src/cli/delete_project.js",
    "add-language": "node ./src/cli/add_language.js",
    "delete-language": "node ./src/cli/delete_language.js"
  },
  "engines": {
    "node": ">=12"
  },
  "author": "Transifex",
  "homepage": "https://github.com/transifex/transifex-delivery",
  "license": "Apache-2.0",
  "dependencies": {
    "@sentry/node": "^5.24.2",
    "aws-sdk": "^2.762.0",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "bull": "^3.18.0",
    "chai-http": "^4.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-prom-bundle": "^6.1.0",
    "glob": "^7.1.6",
    "joi": "^14.3.1",
    "lodash": "^4.17.20",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "nconf": "^0.10.0",
    "nconf-yaml": "^1.0.2",
    "newrelic": "^6.13.1",
    "node-cache": "^5.1.2",
    "prom-client": "^12.0.0",
    "redis": "^3.0.2",
    "slugify": "^1.4.5",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.1.3",
    "nock": "^13.0.4",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "sinon": "^9.0.3",
    "supertest": "^4.0.2"
  }
}
