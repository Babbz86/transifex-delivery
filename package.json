{
  "name": "transifex-delivery",
  "version": "2.5.1",
  "description": "Transifex Content Delivery Service",
  "keywords": [
    "transifex",
    "i18n",
    "l10n",
    "localization"
  ],
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "eslint": "eslint src/",
    "start": "node ./src/index.js",
    "start-web": "node ./src/index.js --only=web",
    "start-worker": "node ./src/index.js --only=worker",
    "start-dev": "node ./node_modules/nodemon/bin/nodemon -L -e js,yml --watch ./src --watch ./config ./src/index.js",
    "start-debugger": "node --inspect-brk=0.0.0.0 ./src/index.js",
    "test": "NODE_ENV=test nyc --reporter=text --all --reporter=html mocha --timeout 5000 --exit \"./tests/**/*.spec.js\""
  },
  "engines": {
    "node": ">=14"
  },
  "author": "Transifex",
  "homepage": "https://github.com/transifex/transifex-delivery",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/storage": "^6.8.0",
    "@sentry/node": "^7.28.1",
    "aws-sdk": "^2.1263.0",
    "axios": "^0.27.2",
    "axios-retry": "^3.3.1",
    "body-parser": "^1.20.1",
    "bull": "^4.10.2",
    "chai-http": "^4.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "express": "^4.18.2",
    "express-prom-bundle": "^6.6.0",
    "express-rate-limit": "^6.7.0",
    "glob": "^8.0.3",
    "ioredis": "^5.2.4",
    "joi": "^14.3.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nconf": "^0.12.0",
    "nconf-yaml": "^1.0.2",
    "newrelic": "^9.7.4",
    "node-cache": "^5.1.2",
    "prom-client": "^14.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^10.2.0",
    "nock": "^13.2.9",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "sinon": "^15.0.1",
    "supertest": "^6.3.3"
  }
}
