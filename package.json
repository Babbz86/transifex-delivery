{
  "name": "transifex-delivery",
  "version": "0.10.2",
  "description": "Transifex Content Delivery Service",
  "keywords": [
    "transifex",
    "i18n",
    "l10n",
    "localization"
  ],
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "eslint": "eslint src/",
    "start": "node ./src/index.js",
    "start-dev": "node ./node_modules/nodemon/bin/nodemon -L -e js,yml --watch ./src --watch ./config ./src/index.js",
    "start-debugger": "node --inspect-brk=0.0.0.0 ./src/index.js",
    "test": "NODE_ENV=test nyc --reporter=text --all --reporter=html mocha --timeout 5000 --exit \"./tests/**/*.spec.js\""
  },
  "engines": {
    "node": ">=14"
  },
  "author": "Transifex",
  "homepage": "https://github.com/transifex/transifex-delivery",
  "license": "Apache-2.0",
  "dependencies": {
    "@sentry/node": "^6.3.0",
    "aws-sdk": "^2.889.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bull": "^3.22.1",
    "chai-http": "^4.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "express-prom-bundle": "^6.3.6",
    "glob": "^7.1.6",
    "joi": "^14.3.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "nconf": "^0.11.2",
    "nconf-yaml": "^1.0.2",
    "newrelic": "^7.3.1",
    "node-cache": "^5.1.2",
    "prom-client": "^13.1.0",
    "redis": "3.0.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.2",
    "nock": "^13.0.11",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "sinon": "^10.0.0",
    "supertest": "^6.1.3"
  }
}
